{
  "name": "ng-infinite-scroll",
  "version": "0.0.1",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "ng": "ng",
    "start": "ng serve --aot --configuration es5",
    "dev": "run-p -r -n start dev:browsersync",
    "dev:browsersync": "npx browser-sync start --no-open --ws -p http://localhost:4200/",
    "build": "ng build --aot",
    "build:prod": "ng build --prod",
    "build:deploy": "ng build --prod --base-href https://deadlyfingers.github.io/ng-infinite-scroll/",
    "test": "ng test",
    "test:coverage": "ng test --code-coverage --watch=false --browsers=ChromeHeadless",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "predeploy": "npm run build:deploy",
    "deploy": "gh-pages -d dist/ng-infinite-scroll"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@ngrx/effects": "^8.6.0",
    "@ngrx/store": "^8.6.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.20",
    "@angular/cli": "~8.3.20",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@ngrx/store-devtools": "^8.6.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "browser-sync": "^2.26.7",
    "codelyzer": "^5.0.0",
    "eslint": "^6.7.2",
    "gh-pages": "^2.1.1",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "jasmine-core": "~3.4.0",
    "jasmine-marbles": "^0.6.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "kind-of": ">=6.0.3",
    "minimist": ">=1.2.2",
    "npm-force-resolutions": "0.0.3",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.0",
    "serialize-javascript": "^2.1.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  },
  "resolutions": {
    "serialize-javascript": "2.1.2"
  },
  "homepage": "https://deadlyfingers.github.io/ng-infinite-scroll"
}
